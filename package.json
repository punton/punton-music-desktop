{
  "name": "PuntonMusic",
  "version": "1.2.4-beta",
  "author": "Punton Music <punton.music@punton.media>",
  "contributors": [
    {
      "name": "PNNutkung",
      "email": "pipatpol.t@ku.th",
      "url": "https://github.com/PNNutkung"
    },
    {
      "name": "pakponj",
      "email": "pakpon.j@ku.th",
      "url": "https://github.com/pakponj"
    },
    {
      "name": "carymono",
      "email": "thanawit.g@ku.th",
      "url": "https://github.com/carymono"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/punton/punton-music-desktop.git"
  },
  "description": "A music player with Machine Learning techniques.",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder --publish always",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:win": "node .electron-vue/build.js && electron-builder --win",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix && electron-builder install-app-deps"
  },
  "build": {
    "productName": "PuntonMusic",
    "copyright": "Copyright Â© 2018 Punton Music",
    "appId": "media.punton.desktop",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*",
      "build/icon.*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.music",
      "icon": "build/icons/icon.png",
      "type": "distribution"
    },
    "win": {
      "publisherName": "Punton Music",
      "legalTrademarks": "Punton Music",
      "icon": "build/icons/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons/icon.png",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "executableName": "Punton Music",
      "synopsis": "A Music Player.",
      "category": "Audio",
      "desktop": "Punton Music"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "punton"
      }
    ]
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.5.1",
    "convnetjs": "^0.3.0",
    "dotenv": "^5.0.0",
    "dynamic-time-warping": "^1.0.0",
    "element-ui": "^2.3.2",
    "lodash": "^4.17.4",
    "music-metadata": "^0.8.8",
    "node-gyp": "^3.6.2",
    "sequelize": "^5.15.1",
    "sqlite3": "^3.1.13",
    "static-kdtree": "^1.0.2",
    "vue": "^2.3.3",
    "vue-awesome": "^3.0.0",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vue-slider-component": "^2.6.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^2.3.1",
    "waveform-data": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.7.3",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "require-dir": "^0.3.0",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
